# Example .env.local for apps/web (place in apps/web/.env.local)
# Replace the values with `dfx canister id <canister>` output after deploy

NEXT_PUBLIC_IC_HOST=http://localhost:8000
NEXT_PUBLIC_FRONTEND_CANISTER_ID=REPLACE_WITH_FRONTEND_CANISTER_ID
NEXT_PUBLIC_HHDAO_CANISTER_ID=REPLACE_WITH_HHDAO_DAO_CANISTER_ID
NEXT_PUBLIC_IDENTITY_CANISTER_ID=REPLACE_WITH_IDENTITY_CANISTER_ID
NEXT_PUBLIC_AUDIT_LOG_CANISTER_ID=REPLACE_WITH_AUDIT_LOG_CANISTER_ID
NEXT_PUBLIC_REVOCATION_CANISTER_ID=REPLACE_WITH_REVOCATION_CANISTER_ID

# Optional runtime flags
NEXT_PUBLIC_ENABLE_ACTOR=true
NEXT_PUBLIC_DFX_NETWORK=local
