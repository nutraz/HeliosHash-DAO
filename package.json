{
  "name": "helioshash-dao",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bash -lc \"pgrep -af dfx >/dev/null || dfx start --background --clean\" && cd frontend && pnpm dev",
    "build": "cd frontend && pnpm build",
    "backend:deploy": "dfx deploy",
    "frontend:deploy": "cd frontend && pnpm build",
    "clean": "dfx stop && rm -rf .dfx frontend/.next frontend/node_modules",
    "generate": "dfx generate",
    "start": "dfx start --background && cd frontend && pnpm start"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@dfinity/agent": "3.4.3",
    "@dfinity/auth-client": "3.4.3"
  }
}
